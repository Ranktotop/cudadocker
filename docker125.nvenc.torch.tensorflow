FROM ranktotop/cudadocker:125_nvenc_torch AS base
    
######################################
###### INSTALL PYTHON PACKAGES #######
######################################
RUN pip3 install --no-cache-dir "tensorflow-hub" \
    "tensorflow==2.18.0" \
    "tensorboard==2.18.0" \
    "numpy<2"
    
ENV TF_ENABLE_ONEDNN_OPTS=0
ENV TF_CPP_MIN_LOG_LEVEL=3